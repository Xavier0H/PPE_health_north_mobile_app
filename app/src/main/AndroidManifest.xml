<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/Theme.Health_north_app"
        tools:targetApi="31">
        <activity
            android:name=".AppointmentListActivity"
            android:exported="false" />
        <activity
            android:name=".DocActivity"
            android:exported="false" />
        <activity
            android:name=".ProfileActivity"
            android:label="Mes Informations" />
        <activity
            android:name=".AppointmentActivity"
            android:label="Mes Rendez-vous" />
        <activity
            android:name=".DocumentActivity"
            android:label="Mes Documents" />
        <activity
            android:name=".DashBoardActivity"
            android:label="Tableaux de Bord" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateAccountActivity"
            android:exported="false" />
    </application>

</manifest>